{
  "name": "@framedev/cli",
  "version": "0.3.0",
  "description": "Frame: local AI code generation agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "frame": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "coding-agent",
    "local-llm",
    "ai-assistant",
    "code-generation"
  ],
  "dependencies": {
    "@framedev/tui": "workspace:*",
    "chalk": "^5.6.2",
    "diff": "^8.0.3",
    "ollama": "^0.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "tsx": "^4.21.0"
  }
}